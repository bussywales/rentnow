    "next": "^16.0.7",
    "openai": "^6.9.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-leaflet": "^5.0.0",
    "stripe": "^16.10.0",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
