  "engines": {
    "node": ">=20.9.0"
  },
  "engineStrict": true,
  "scripts": {
    "preinstall": "node ./scripts/check-node.mjs",
    "dev": "next dev",
    "build": "next build",
    "build:ci": "node ./scripts/run-build-ci.mjs",
    "start": "next start",
    "lint": "eslint",
    "lint:ci": "node ./scripts/run-lint-ci.mjs",
    "typecheck": "tsc --noEmit --project tsconfig.typecheck.json",
