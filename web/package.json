{
  "name": "rentnow",
  "version": "0.2.19",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "engineStrict": true,
  "scripts": {
    "preinstall": "node ./scripts/check-node.js",
    "dev": "next dev",
    "build": "BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true next build",
    "build:ci": "node ./scripts/run-build-ci.mjs",
    "start": "next start",
    "lint": "node -e \"require('fs').mkdirSync('test-results',{recursive:true})\" && BROWSERSLIST_IGNORE_OLD_DATA=true BASELINE_BROWSER_MAPPING_IGNORE_OLD_DATA=true eslint",
    "lint:ci": "node ./scripts/run-lint-ci.mjs",
    "guard:no-server-getsession": "node ./scripts/guard-no-server-getsession.mjs",
    "test:ci": "npm run guard:no-server-getsession && npm run lint && npm run test && npm run build",
    "typecheck": "tsc --noEmit --project tsconfig.typecheck.json",
    "doctor": "node ./scripts/doctor.mjs",
    "test": "tsx --test tests/unit/*.test.ts",
    "check:migrations": "node ./scripts/check-migration-timestamps.mjs",
    "setup": "npm install && npx playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:e2e:ci": "node ./scripts/run-test-e2e-ci.mjs",
    "test:e2e:cleanup": "node scripts/cleanup-playwright.mjs"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "docx": "^9.5.1",
    "embla-carousel-react": "^8.6.0",
    "leaflet": "^1.9.4",
    "next": "^16.0.7",
    "openai": "^6.9.1",
    "pdf-lib": "^1.17.1",
    "react-day-picker": "^9.6.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-leaflet": "^5.0.0",
    "stripe": "^16.10.0",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9",
    "eslint-config-next": "^16.0.7",
    "playwright": "^1.49.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
